.venv/
venv/
env/
.env/

# Testing and coverage
.pytest_cache/
.mypy_cache/
.coverage
htmlcov/
*.cover

# Build artifacts
*.egg-info/
dist/
build/
*.egg

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db
