{
  "workflow_data": {
    "credential": {
      "access_key_id": "${STORAGE_ACCESS_KEY_ID}",
      "endpoint": "${WORKFLOW_DATA_ENDPOINT}",
      "region": "${AZURE_REGION}"
    },
    "base_url": "${WORKFLOW_BASE_URL}"
  },
  "workflow_log": {
    "credential": {
      "access_key_id": "${STORAGE_ACCESS_KEY_ID}",
      "endpoint": "${WORKFLOW_LOG_ENDPOINT}",
      "region": "${AZURE_REGION}"
    }
  },
  "workflow_app": {
    "credential": {
      "access_key_id": "${STORAGE_ACCESS_KEY_ID}",
      "endpoint": "${WORKFLOW_APP_ENDPOINT}",
      "region": "${AZURE_REGION}"
    }
  },
  "workflow_info": {
    "tags": [],
    "max_name_length": 64
  },
  "backend_images": {
    "credential": {
      "registry": "nvcr.io",
      "username": "$$oauthtoken",
      "auth": "${NGC_TOKEN}"
    }
  },
  "credential_config": {
    "disable_registry_validation": [
      "ghcr.io",
      "${ACR_LOGIN_SERVER}"
    ],
    "disable_data_validation": []
  }
}
